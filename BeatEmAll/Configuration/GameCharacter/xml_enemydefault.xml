		 			<vertice x="350"    y="250"/>
		 			<vertice x="350"    y="-250"/>
		 		</shape>
		 		<filter category="CAT_ENEMY_SENSOR" maskBits="CAT_PLAYER" />
		 	</fixture>
		 </fixtures>
	</physics>

	<graphic>
		<origin x="14" y="21" />
		<followRotation>true</followRotation>
		<representation repType="sprite">
			<path>assets\\Robot 1\\robot1_hold.png</path>
			<scale factorX=".6" factorY=".6"/>
		</representation>
	</graphic>

	<userData>
		<velocity>3</velocity>
		<health>100</health>
	</userData>
