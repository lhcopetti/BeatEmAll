<EnemyDefault>
	<physics>
		<body>
			<type>dynamic</type>
			<bullet>true</bullet>
		</body>

		 <fixtures>
		 	<fixture>
		 		<density>1f</density>
		 		<restitution>.5f</restitution>
		 		<shape type="circle">
		 			<radius>15.0</radius>
		 			<position x="0" y="0" />
		 		</shape>
		 		<filter category="CAT_ENEMY" maskBits="CAT_PLAYER|CAT_ENEMY|CAT_BOUNDARY|CAT_BULLET" />
		 	</fixture>
		 	<fixture>
		 		<density>0</density>
		 		<restitution>.5f</restitution>
